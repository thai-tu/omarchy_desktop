{
  "reload_style_on_change": true,
  "layer": "top",
  "position": "top",
  "mod": "dock",
  "margin-top": 10,
  "height": 36,
  "margin-left": 10,
  "margin-right": 10,
  "margin-bottom": 0,
 
    "group/left1": {
    "orientation": "inherit",
    "modules": ["custom/omarchy", "clock", "custom/weather"]
    },

    "group/left2": {
    "orientation": "inherit",
    "modules": ["hyprland/workspaces"]
    },

    "group/left3": {
    "orientation": "inherit",
    "modules": [
      "idle_inhibitor",
      "custom/screenrecording-indicator",
      "custom/updatespacman",
      "custom/update"
      ]
    },

    "group/left4": {
    "orientation": "inherit",
    "modules": ["mpris"]
    },

    "modules-left": [
    "group/left1",
    "group/left2",
    "group/left3",
    "group/left4"
    ],

    "modules-left": ["group/left1", "group/left2", "group/left3", "group/left4", "group/left5"],
    "modules-center": ["hyprland/window"],
    "modules-right": ["group/right1", "group/right2"],
    "group/right1": {
    "orientation": "inherit",
    "modules": ["tray"]
    },

    "group/right2": {
    "orientation": "inherit",
    "modules": [
    "bluetooth",
    "pulseaudio#output",
    "pulseaudio#input",
    "network",
    "memory",
    "temperature#gpu",
    "temperature",
    "cpu"
      ]
    },

    "hyprland/workspaces": {
    "format": "{icon}",
    "format-icons": {
      "1": "‰∏Ä",
      "2": "‰∫å",
      "3": "‰∏â",
      "4": "Âõõ",
      "5": "‰∫î",
      "6": "ÂÖ≠"
      },
    "persistent-workspaces": {
      "*": [1, 2, 3, 4, 5, 6]
      }
    },

    "custom/omarchy": {
    "format": "Û∞ñÇ ",
    "on-click": "omarchy-menu",
    "on-click-right": "xdg-terminal-exec",
    "tooltip-format": "Omarchy Menu\n\nSuper + Alt + Space"
    },

    "custom/update": {
    "format": "ÔÄ°",
    "exec": "omarchy-update-available",
    "on-click": "omarchy-launch-floating-terminal-with-presentation omarchy-update",
    "tooltip-format": "Omarchy update available",
    "signal": 7,
    "interval": 21600
    },

    "custom/updatespacman": {
    "format": "{}{icon}",
    "return-type": "json",
    "format-icons": {
      "has-updates": " Û∞èñ",
      "updated": ""
      },
    "exec-if": "which waybar-module-pacman-updates",
    "exec": "waybar-module-pacman-updates --no-zero-output"
    },

    "hyprland/window": {
    "format": "{title} - {class}",
    "max-length": 85,
    "rewrite": {
      " - ": "", // if no window is focused, a single dash is displayed, this removes it
      "(.*?) - Chromium - chromium": " Ôâ®  $1 ",
      "(.*chrome)...(.*)": " Ôâ®  $1 ",
      "(.*) - Obsidian - .* - obsidian": " Ôâä  $1 ",
      "(.*nvim)...(.*)": " ÔçØ  $2 ",
      "(.*code) - (.*)": " Û∞®û  $1 ",
      "^.*btop( .*|$)": " Û∞ß® BTOP $1 ",
      "^.*cmatrix( .*|$)": " Û∞ò® MATRIX $1 ",
      "(.*) - Spotify": " ÔÜº $1 ",
      "(.*) - com.mitchellh.ghostty": " Û∞ä† $1 ",
      "(.*) - Alacritty": " Ôíâ  $1 ",
      "(.*) - BeeperTexts": " Û∞ªû  $1 ",
      "(.*Discord)...(.*).. (.*)": " Ôáø  $2 ",
      "(.*) - org.gnome.Nautilus": " Û∞£Ü  $1 ",
      "(.*) - fish": " Ôíâ  [$1] ",
      "(.*Steam)...(.*)": " ÔÜ∂  $1",
      "(.*imv)...(.*).. (.*)": " Ôì•  $2",
      "(.*) - com.obsproject.Studio": " Ôëá $1 "
      }
    },

    "cpu": {
    "interval": 2,
    "format": "Û∞çõ {usage}%",
    "on-click": "xdg-terminal-exec --app-id=com.omarchy.Btop --title='Btop' -e btop"

    },

    "memory": {
    "interval": 2,
    "format": " ÓøÖ  {used:.1f}G",
    "tooltip": true,
    "tooltip-format": "Used:\t{used:5.2f}GiB\nFree:\t{avail:5.2f}GiB\nTotal:\t{total:5.2f}GiB",
    "on-click": "xdg-terminal-exec --app-id=com.omarchy.Btop --title='Btop' -e btop"

    },

    "clock": {
    "format": "ÔÑ≥ {:%H:%M %a} ",
    "format-alt": "ÔÑ≥ {:%d/%m/%Y  %H:%M:%S}",
    "tooltip-format": "<span>{calendar}</span>",
    "calendar": {
      "mode": "month",
      "weeks-pos": "right",
      "mode-mon-col": 3,
      "on-click-right": "mode",
      "format": {
        "mouth": "<span color='#ffead3'><b>{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b>{}</b></span>"
        }
      },
    "on-click-right": "omarchy-launch-floating-terminal-with-presentation omarchy-tz-select"
    },

    "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "Û∞õä",
      "deactivated": "Û∞æ´"
      }
    },

    "network": {
    "format-icons": [
      "Û∞§Ø",
      "Û∞§ü",
      "Û∞§¢",
      "Û∞§•",
      "Û∞§®"
      ],
    "format": "{icon}",
    "format-wifi": "{icon}  {essid}",
    "format-ethernet": "Û∞ÄÇ",
    "format-disconnected": "Û∞ñ™",
    "tooltip-format-wifi": "{essid} ({frequency} GHz)\n‚á£{bandwidthDownBytes}  ‚á°{bandwidthUpBytes}",
    "tooltip-format-ethernet": "‚á£{bandwidthDownBytes}  ‚á°{bandwidthUpBytes}",
    "tooltip-format-disconnected": "Disconnected",
    "interval": 3,
    "nospacing": 1,
    "on-click": "omarchy-launch-wifi"
    },
  
    "bluetooth": {
    "format": "Ôäî",
    "format-disabled": "Û∞Ç≤",
    "format-off": "Û∞Ç≤",
    "format-connected": "Û∞Ç±",
    "tooltip-format": "Devices connected: {num_connections}",
    "on-click": "omarchy-launch-bluetooth"
    },

    "custom/weather": {
        "format": "{}¬∞",
        "tooltip": true,
        "interval": 1800,
        "exec": "wttrbar --location Melbourne,Australia",
        "return-type": "json"
    },
    "pulseaudio#input": {
    "format": "{format_source}",
    "format-source": "ÔÑ∞ {volume}%",
    "format-source-muted": "ÔÑ±  {volume}%",
    "tooltip-format": "{source_desc}",
    "on-click": "omarchy-launch-or-focus-tui wiremix",
    "on-click-right": "pamixer --default-source -t",
    "on-scroll-up": "pamixer --default-source -i 5",
    "on-scroll-down": "pamixer --default-source -d 5",
    "max-volume": 100
    },

    "pulseaudio#output": {
    "format": "{icon}  {volume}%",
    "on-click": "omarchy-launch-or-focus-tui wiremix",
    "on-click-right": "pamixer -t",
    "tooltip-format": "{source_desc}",
    "scroll-step": 5,
    "format-muted": "Óª®  {volume}%",
    "format-icons": {
      "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
      }
    },

    "tray": {
    "icon-size": 12,
    "spacing": 10
    },

    "mpris": {
    "format": "Û∞ãé {artist} - {title}",
    "format-paused": "<i>{status_icon} {artist}</i>",
    "max-length": 20,
    "player-icons": {
      "default": "‚è∏",
      "mpv": "üéµ"
     },
    "status-icons": {
      "paused": "Û∞ãê"  
      }
    // "ignored-players": ["firefox"]
    },

    "custom/screenrecording-indicator": {
    "on-click": "omarchy-cmd-screenrecord",
    "exec": "$OMARCHY_PATH/default/waybar/indicators/screen-recording.sh",
    "signal": 8,
    "return-type": "json"
    },

    "temperature": {
    "interval": 5,
    "hwmon-path": "/sys/class/hwmon/hwmon5/temp1_input",
    "format": "Ôãâ CPU {}¬∞C",
    "warning-threshold": 75,
    "critical-threshold": 90,
    "on-click": "xdg-terminal-exec --app-id=com.omarchy.Btop --title='Btop' -e btop"

    },
    
    "temperature#gpu": {
    "interval": 5,
    "hwmon-path": "/sys/class/hwmon/hwmon4/temp1_input",
    "format": "Û∞¢Æ GPU {temperatureC}¬∞C",
    "warning-threshold": 80,
    "critical-threshold": 95,
    "on-click": "xdg-terminal-exec --app-id=com.omarchy.Btop --title='Btop' -e btop"

    }
}
